a <-vector("numeric")
if (is.numeric(num)== TRUE){
for (i in 1:54){
n <- xx[[i]]
n[,y] <- suppressWarnings(as.numeric(n[,y]))
orden <- order(n[,y],n[,2])
hospitales <- n[orden,2]
nn <- hospitales[num]
a <- c(a,nn)
}
}else if (num == "peor"){
num <- 1
for (i in 1:54){
