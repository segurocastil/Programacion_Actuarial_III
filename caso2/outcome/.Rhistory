n[,y] <- suppressWarnings(as.numeric(n[,y]))
orden <- order(n[,y],n[,2],decreasing = T)
hospitales <- n[orden,2]
nn <- hospitales[num]
a <- c(a,nn)
}
}
valores <- data.frame(a,names(xx))
names(valores) <- c("hospital","estado")
rownames(valores) <- names(xx)
valores
}
